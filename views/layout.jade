    #wrapper
        // Navigation
        nav.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
          // Brand and toggle get grouped for better mobile display
          .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-ex1-collapse')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(href='index.html') ECEP
          // Top Menu Items
          ul.nav.navbar-right.top-nav
            li.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.fa.fa-bell
                b.caret
              ul.dropdown-menu.alert-dropdown
                li
                  a(href='#')
                    | Alert Name 
                    span.label.label-default Alert Badge
                li
                  a(href='#')
                    | Alert Name 
                    span.label.label-primary Alert Badge
            li.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                i.fa.fa-user
                |  Labhesh 
                b.caret
              ul.dropdown-menu
                li
                  a(href='#')
                    i.fa.fa-fw.fa-user
                    |  Profile
                li.divider
                li
                  a(href='#')
                    i.fa.fa-fw.fa-power-off
                    |  Log Out
          // Sidebar Menu Items - These collapse to the responsive navigation menu on small screens
          .collapse.navbar-collapse.navbar-ex1-collapse
            ul.nav.navbar-nav.side-nav
              li.active
                a(href='index.html')
                  i.fa.fa-fw.fa-dashboard
                  |  Dashboard
              li
                a(href='charts.html')
                  i.fa.fa-fw.fa-bar-chart-o
                  |  Charts
              li
                a(href='tables.html')
                  i.fa.fa-fw.fa-table
                  |  Tables
              li
                a(href='javascript:;', data-toggle='collapse', data-target='#demo')
                  i.fa.fa-fw.fa-arrows-v
                  |  Forms 
                  i.fa.fa-fw.fa-caret-down
                ul#demo.collapse
                  li
                    a(href='#') Container Form
                  li
                    a(href='#') Application Form
          // /.navbar-collapse            
block content
